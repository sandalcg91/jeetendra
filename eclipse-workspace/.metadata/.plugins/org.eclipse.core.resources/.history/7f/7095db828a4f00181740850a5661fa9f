package com.per_phone.practice;

public class Person {

	private Integer pid;
	private String pname;
	private Set phone;
	public Integer getPid() {
		return pid;
	}
	public void setPid(Integer pid) {
		this.pid = pid;
	}
	public String getPname() {
		return pname;
	}
	public void setPname(String pname) {
		this.pname = pname;
	}
	public Set getPhone() {
		return phone;
	}
	public void setPhone(Set phone) {
		this.phone = phone;
	}
	
}
